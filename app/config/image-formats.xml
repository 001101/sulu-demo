<?xml version="1.0" encoding="UTF-8"?>
<formats xmlns="http://schemas.sulu.io/media/formats"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://schemas.sulu.io/media/formats http://schemas.sulu.io/media/formats-1.0.xsd">
    <format>
        <name>1600x</name>
        <commands>
            <command>
                <action>scale</action>
                <parameters>
                    <parameter name="x">1600</parameter>
                    <parameter name="mode">inset</parameter>
                </parameters>
            </command>
        </commands>
        <options>
            <option name="jpeg_quality">100</option>
            <option name="png_compression_level">4</option>
        </options>
    </format>
