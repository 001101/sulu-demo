<section class="partner"  rel="blocks" typeof="block">
    <h2 property="title" class="partner-item--heading heading--h2--border">
        {{ partner.title }}
    </h2>

    <div property="partnerData"
         class="partner-carousel js-partner-carousel"
         data-slick='{"slidesToShow": 5, "slidesToScroll": 5}'>
        {% for item in partner.partnerData %}
            {% for image in item.images %}
                <div class="partner-carousel-item">
                    <img src="{{ image.thumbnails['140x70'] }}" alt="{{ image.name }}">
                </div>
            {% endfor %}
        {% endfor %}
    </div>
</section>